    <content language="ENU" version="2.0" approvedVersion="8">
      <rationaleoptlist />
      <stem><![CDATA[<p style="color:#000000; ">This figure <span id="item_1991_TAG_3" class="its-tag" data-tag="word" data-tag-boundary="start" data-word-index="1"></span>is made by joining<span class="its-tag" data-tag-ref="item_1991_TAG_3" data-tag-boundary="end"></span> two rectangles.</p><p style="color:#000000; ">&#xA0;</p><p style="color:#000000; "><img id="item_1991_Stem:(d=png16malpha)" src="item_1991_v8_Stem_png16malpha.png" width="250" height="175" style="vertical-align:baseline;" /></p><p style="color:#000000; ">&#xA0;</p><p style="color:#000000; "><span id="item_1991_TAG_4" class="its-tag" data-tag="word" data-tag-boundary="start" data-word-index="2"></span>Enter<span class="its-tag" data-tag-ref="item_1991_TAG_4" data-tag-boundary="end"></span> the area, in square feet, of the <span id="item_1991_TAG_1_BEGIN">figure</span>.</p>]]></stem>
      <rubriclist>
        <rubric scorepoint="">
          <name>
        Rubric </name>
          <val><![CDATA[<p style="color:#000000; "><span style="font-weight:bold; ">Exemplar:</span> 20</p><p style="color:#000000; ">&#xA0;</p><p style="color:#000000; ">For this item, a full-credit response includes</p><p style="color:#000000; ">&#xA0;</p><ul style="padding-left:1.5000em; "><li><p style="color:#000000; ">The student enters the number 20, or any equivalent number. (1 point).</p></li></ul>]]></val>
        </rubric>
